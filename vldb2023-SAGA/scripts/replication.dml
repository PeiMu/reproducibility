#read file

F = read($1, data_type="frame", format="csv", header=TRUE, naStrings= ["NA", "null","  ","NaN", "_nan_", "", "?", " ", "nan", "inf", "NAN", "99999", "99999.00"], sep=",");


Fsingle = F

for(i in 2:110)
{
  out = $2+i+".csv"
	F = rbind(F, Fsingle)
  print("file written "+out)
  print("file rows: "+nrow(F))
	write(F,out, format="csv", header=TRUE)

}